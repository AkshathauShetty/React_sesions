
import { buildTimeValue } from "@testing-library/user-event/dist/utils";
import { Component } from "react";
import React from "react";


class Logincontrolclass extends Component{

    constructor(props){
        super(props);
        this.state={
            isLoggedIn : "true"
        }
    }

    handleLogin(){
        this.setState({
            isLoggedIn:true
        })
    }

     handleLogout(){
        this.setState({
            isLoggedIn:false
        })
    }



}